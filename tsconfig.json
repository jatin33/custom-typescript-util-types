{
  "compilerOptions": {
    "module": "commonjs",
    "strict": true,
    "target": "es5",
    "declaration": true,
    "outDir": "./dist",
    "lib": ["dom", "es5"],
    "pretty": true,
    "removeComments": true,
    "sourceMap": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "include": ["src/**/*"]
}
